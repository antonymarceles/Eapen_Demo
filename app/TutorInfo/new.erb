<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @tutorinfo.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @tutorinfo.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="tutorinfo[name]" class="fieldLabel">Name</label>
            <input type="text" id="tutorinfo[name]" name="tutorinfo[name]" <%= placeholder("Name") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tutorinfo[zip]" class="fieldLabel">Zip</label>
            <input type="text" id="tutorinfo[zip]" name="tutorinfo[zip]" <%= placeholder("Zip") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="tutorinfo[subjects]" class="fieldLabel">Subjects</label>
            <input type="text" id="tutorinfo[subjects]" name="tutorinfo[subjects]" <%= placeholder("Subjects") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
