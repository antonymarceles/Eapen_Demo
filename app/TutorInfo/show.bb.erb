<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @tutorinfo.name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @tutorinfo.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @tutorinfo.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Zip</td>
        <td class="itemValue"><%= @tutorinfo.zip%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Subjects</td>
        <td class="itemValue"><%= @tutorinfo.subjects%></td>
      </tr>
      
    </table>
  </div>

</div>
