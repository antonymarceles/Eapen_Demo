<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @studentinfo.name %></h1>
    <%= link_to "Edit", :action => :edit, :id => @studentinfo.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @studentinfo.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Zip</td>
        <td class="itemValue"><%= @studentinfo.zip%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Exam</td>
        <td class="itemValue"><%= @studentinfo.exam%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Subject</td>
        <td class="itemValue"><%= @studentinfo.subject%></td>
      </tr>
      
    </table>
  </div>

</div>
