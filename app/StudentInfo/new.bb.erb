<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New StudentInfo</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @studentinfo.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Name: </td>
                <td class="itemValue"><input type="text" name="studentinfo[name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Zip: </td>
                <td class="itemValue"><input type="text" name="studentinfo[zip]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Exam: </td>
                <td class="itemValue"><input type="text" name="studentinfo[exam]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Subject: </td>
                <td class="itemValue"><input type="text" name="studentinfo[subject]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
